{
    "name": "mkosolofski/houseseats-monitor",
    "license": "MIT",
    "type": "library",
    "description": "Symfony 3 bundle for monitoring new active shows on houseseats.com",
    "autoload": {
        "psr-4": { "MKosolofski\\HouseSeats\\MonitorBundle\\": "" }
    },
    "require": {
        "php": ">=7.0.0",
        "symfony/symfony": "3.0.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "phake/phake": "^2.3"
    },
    "config": {
        "bin-dir": "bin"
    }
}
